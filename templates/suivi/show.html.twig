{% extends 'base.html.twig' %}

{% block title %}Suivi{% endblock %}

{% block body %}
    <h1>Suivi</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>IdSuivi</th>
                <td>{{ suivi.idSuivi }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ suivi.nom }}</td>
            </tr>
            <tr>
                <th>Prenom</th>
                <td>{{ suivi.prenom }}</td>
            </tr>
            <tr>
                <th>Civilite</th>
                <td>{{ suivi.civilite }}</td>
            </tr>
            <tr>
                <th>DateSuivi</th>
                <td>{{ suivi.dateSuivi ? suivi.dateSuivi|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Fonction</th>
                <td>{{ suivi.fonction }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('suivi_index') }}">back to list</a>

    <a href="{{ path('suivi_edit', {'idSuivi': suivi.idSuivi}) }}">edit</a>

    {{ include('suivi/_delete_form.html.twig') }}
{% endblock %}
