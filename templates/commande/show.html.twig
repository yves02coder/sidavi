{% extends 'base.html.twig' %}

{% block title %}Commande{% endblock %}

{% block body %}
    <h1>Commande</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>IdCommande</th>
                <td>{{ commande.idCommande }}</td>
            </tr>
            <tr>
                <th>ClientId</th>
                <td>{{ commande.clientId }}</td>
            </tr>
            <tr>
                <th>DateCommande</th>
                <td>{{ commande.dateCommande ? commande.dateCommande|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>QtePoussin</th>
                <td>{{ commande.qtePoussin }}</td>
            </tr>
            <tr>
                <th>TotalCommande</th>
                <td>{{ commande.totalCommande }}</td>
            </tr>
            <tr>
                <th>MoyenPayement</th>
                <td>{{ commande.moyenPayement }}</td>
            </tr>
            <tr>
                <th>EtatCommande</th>
                <td>{{ commande.etatCommande }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('commande_index') }}">back to list</a>

    <a href="{{ path('commande_edit', {'idCommande': commande.idCommande}) }}">edit</a>

    {{ include('commande/_delete_form.html.twig') }}
{% endblock %}
